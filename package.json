{
  "name": "chrome-extension-typescript-starter",
  "version": "1.0.0",
  "description": "chrome-extension-typescript-starter",
  "main": "index.js",
  "scripts": {
    "dev-popup": "webpack serve --config webpack.preview.popup.js",
    "dev-options": "webpack serve --config webpack.preview.options.js",
    "dev-app": "webpack serve --config webpack.preview.app.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "build": "rm -rf dist && webpack --config webpack.prod.js",
    "clean": "rimraf dist",
    "test": "npx jest",
    "style": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chibat/chrome-extension-typescript-starter.git"
  },
  "dependencies": {
    "@types/tinycolor2": "^1.4.6",
    "clsx": "^2.1.1",
    "css-loader": "^7.1.1",
    "postcss-loader": "^8.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-haiku": "^2.3.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "tinycolor2": "^1.6.0",
    "webpack-dev-server": "4.6.0",
    "wouter": "^3.1.3"
  },
  "devDependencies": {
    "@types/chrome": "0.0.158",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
