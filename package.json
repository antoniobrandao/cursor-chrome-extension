    "glob": "^7.1.6",
    "jest": "^29.5.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2 ",
    "ts-jest": "^29.1.0",
    "ts-loader": "^8.0.0",
    "typescript": "^5.0.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.0.0",
    "webpack-merge": "^5.0.0"
  }
